.nav .container a {
  color: #5a5a5a;
  font-size: 11px;
  font-weight: bold;
  padding: 14px 10px;
  text-transform: uppercase;
}


.jumbotron {
  background-image: url('pics/breadboard.png');
  height: 500px;
  background-repeat: no-repeat;
  background-size: cover;
  color: #fff;
  font-family: 'Shift', sans-serif;
}

.jumbotron h1 {
  color: #fff;
  font-size: 48px;
  font-family: 'Shift', sans-serif;
  font-weight: bold;
}

.jumbotron p {
  font-size: 20px;
  font-weight: bold;
  color: #fff;
}

th {
  font-size: 14pt;
  color: #fff;
  padding: 5px;
  text-align: center;
  font-weight: bold;
}

td {
  font-size: 14pt;
  color: #fff;
  padding: 5px;
}

.learn-more {
    background-color: #f7f7f7;
}

.learn-more h3 {
    font-family: 'Shift', sans-serif;
    font-size: 18px;
    font-weight: bold;
}

.learn-more a {
    color: #00b0ff;
}

.nav li {
    display: inline;
}

.jumbotron .container {
    position: relative;
    top: 10px;
}

span.dataValue {
	color: #aa0000;
}

input,select,option {
	color: #000;
}

label {
	color: #fff;
}
